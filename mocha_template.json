{
    "template": "mocha-*",
    "mappings": {
        "_default_": {
            "properties": {
                "timestamp": {
                    "type": "date",
                    "index": "not_analyzed",
                    "format": "yyyy-MM-dd HH:mm:ss",
                    "store": "yes"
                },
                "platform": {
                    "type": "integer",
                    "index": "not_analyzed"
                },
                "model": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "resultCode": {
                    "type": "integer",
                    "index": "not_analyzed"
                },
                "did": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "parameters": {
                    "type": "object",
                    "index": "not_analyzed",
                    "dynamic": true
                },
                "sys_ver": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "channel": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "cost": {
                    "type": "integer",
                    "index": "not_analyzed"
                },
                "uri": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "app_ver": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "ip": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "exception": {
                    "type": "string",
                    "index": "not_analyzed"
                }
            }
        }
    }
}